{
  "config": {
    "order": 60,
    "category": "general",
    "description": "manage Monaca configuration",
    "longDescription": [
      "Manage proxy and API endpoint configuration"
    ],
    "usage": [
      "monaca config [<options>]"
    ],
    "options": [
      [
        "proxy <url>",
        "If <url> is unset, the current proxy server will be displayed"
      ],
      [
        "endpoint <url>",
        "If <url> is unset, the current API endpoint will be displayed"
      ],
      [
        "",
        ""
      ],
      [
        "--help",
        "Display config helper"
      ],
      [ "--reset",
        "Reset to the default value"
      ]
    ],
    "examples": [
      "monaca config --help\n",
      "monaca config proxy",
      "monaca config proxy http://my.proxy.com:8080",
      "monaca config proxy --reset\n",
      "monaca config endpoint",
      "monaca config endpoint my.endpoint.com",
      "monaca config endpoint --reset"
    ]
  }
}
